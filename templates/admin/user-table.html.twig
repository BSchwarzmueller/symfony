<h2>Users</h2>
<table style="min-width:400px">
    <tr>
        <th>Email</th>
        <th>Role</th>
        <th>Aktion</th>
    </tr>
    {% for user in users %}
        <tr>
            <td>{{ user.email }}</td>
            <td>
                {% for role in user.roles %}
                    [{{ role|slice(5) }}]
                {% endfor %}
            </td>
        </tr>
    {% endfor %}
</table>
